{
  "name": "time-keeper",
  "displayName": "Time Keeper",
  "description": "This extension allows the user to keep track of their time directly inside of VScode and output the time into a timesheet for tracking",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView: TimeCheaters"
  ],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "explorer": [
    { 
      "id": "TimeCheaters",
      "name": "TimeCheaters",
      "icon": "media/dep.svg",
      "contextualTitle": "TimeCheaters"
    }
  ]},

    "commands": [
      {
        "command": "time-keeper.start",
        "title": "Start"
      },
      {
        "command": "time-keeper.stop",
        "title": "Stop"
      },
      {
        "command": "time-keeper.saveReset",
        "title": "Stop & Save"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
